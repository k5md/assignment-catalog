{
  "name": "assignment-catalog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "karma start",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 server.js",
    "start": "cross-env NODE_ENV=production electron ./app/main/main",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./app/main/main",
    "dev1": "npm run hot-server --  --start-hot",
    "dev": "cross-env HOT=1 webpack-dev-server --mode development --hot --progress --colors --port 3000 ",
    "build": "webpack -p --progress --colors",
    "build-native": "electron-rebuild -m ./app",
    "package-win": "npm run build && build --win --ia32",
    "package-mac": "npm run build && build --mac",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "generate-data": "ts-node --project ./tsconfig.json ./prepare.ts"
  },
  "build": {
    "productName": "electron",
    "appId": "com.gsof.app",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "main/",
      "app.html",
      "node_modules/",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/faker": "^4.1.5",
    "@types/jasmine": "^3.4.0",
    "@types/karma": "^3.0.3",
    "@types/node": "^12.7.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router": "^5.0.3",
    "@types/webpack": "^4.39.1",
    "antd": "^3.22.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "date-fns": "^2.0.1",
    "electron": "^6.0.4",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "^3.4.0",
    "jasmine-enzyme": "^7.1.0",
    "karma": "^4.2.0",
    "karma-electron": "^6.3.0",
    "karma-jasmine": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-webpack": "^4.0.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mobx-react": "^6.1.3",
    "mobx-react-devtools": "^6.0.3",
    "postcss": "^7.0.5",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-reporter": "^6.0.0",
    "postcss-url": "^8.0.0",
    "react": "^16.9.0",
    "react-hot-loader": "^4.3.12",
    "react-router": "^5.0.1",
    "style-loader": "^1.0.0",
    "ts-import-plugin": "^1.6.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "mobx": "^5.5.2",
    "mobx-react-router": "^4.0.5",
    "mobx-state-tree": "^3.14.1",
    "react-dom": "^16.6.0",
    "react-virtualized": "^9.21.1"
  }
}
